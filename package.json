{
  "name": "livelyicons",
  "version": "1.1.0",
  "description": "A comprehensive library of animated icons powered by Lively for React",
  "homepage": "https://livelyicons.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/livelyicons/icons.git"
  },
  "bugs": {
    "url": "https://github.com/livelyicons/icons/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "livelyicons": "./bin/livelyicons.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./static": {
      "types": "./dist/static.d.ts",
      "import": "./dist/static.mjs",
      "require": "./dist/static.js"
    },
    "./css": {
      "types": "./dist/css.d.ts",
      "import": "./dist/css.mjs",
      "require": "./dist/css.js"
    }
  },
  "files": [
    "dist",
    "bin",
    "src/icons",
    "src/static",
    ".vscode",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:lib": "tsup",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "generate:types": "npx tsx scripts/generate-types.ts",
    "benchmark": "npx tsx scripts/benchmark-bundle-size.ts",
    "prepublishOnly": "pnpm run generate:types && pnpm run build:lib"
  },
  "keywords": [
    "react",
    "icons",
    "motion",
    "animation",
    "components",
    "animated-icons",
    "framer-motion",
    "svg-icons",
    "rsc",
    "server-components",
    "css-animations"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "motion": ">=11.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "motion": {
      "optional": true
    }
  },
  "dependencies": {
    "@vercel/analytics": "^1.6.1",
    "fuse.js": "^7.1.0",
    "next": "^15.0.0",
    "tailwindcss": "^4.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "esbuild": "^0.24.2",
    "motion": "^12.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.7.0"
  }
}
